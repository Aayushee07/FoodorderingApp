<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFE5BF"
    tools:context=".MenuCard1">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="100dp"
                android:background="#DB5C00"
                android:fontFamily="@font/archivo_black"
                android:gravity="center"
                android:text="Menu"
                android:textColor="#FAF7F7"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="379dp"
                android:layout_height="102dp"
                android:layout_gravity="center"
                android:layout_marginTop="-70dp"
                android:background="@drawable/backgroundeditview"
                android:backgroundTint="#FFFFFF"
                android:fontFamily="@font/archivo_black"
                android:gravity="center"
                android:text="                BREAKFAST"
                android:textColor="#DB5C00"
                android:textSize="28sp" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="163dp"
                android:layout_height="189dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="-145dp"
                app:srcCompat="@drawable/breakfast" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="379dp"
                android:layout_height="102dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:background="@drawable/backgroundeditview"
                android:backgroundTint="#FFFFFF"
                android:fontFamily="@font/archivo_black"
                android:gravity="center|left"
                android:text="      JUICES"
                android:textColor="#DB5C00"
                android:textSize="28sp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="135dp"
                android:layout_height="107dp"
                android:layout_marginStart="262dp"
                android:layout_marginTop="-104dp"
                app:srcCompat="@drawable/juices" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="379dp"
                android:layout_height="102dp"
                android:layout_gravity="center"
                android:layout_marginTop="40dp"
                android:background="@drawable/backgroundeditview"
                android:backgroundTint="#FFFFFF"
                android:fontFamily="@font/archivo_black"
                android:gravity="center"
                android:text="          SALADS"
                android:textColor="#DB5C00"
                android:textSize="28sp" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="278dp"
                android:layout_height="102dp"
                android:layout_marginStart="-75dp"
                android:layout_marginTop="-103dp"
                app:srcCompat="@drawable/salad" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="379dp"
                android:layout_height="102dp"
                android:layout_gravity="center"
                android:layout_marginTop="40dp"
                android:background="@drawable/backgroundeditview"
                android:backgroundTint="#FFFFFF"
                android:fontFamily="@font/archivo_black"
                android:gravity="center|left"
                android:text=" MEAL BOWLS"
                android:textColor="#DB5C00"
                android:textSize="26sp" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="158dp"
                android:layout_height="235dp"
                android:layout_marginStart="237dp"
                android:layout_marginTop="-170dp"
                app:srcCompat="@drawable/img" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
